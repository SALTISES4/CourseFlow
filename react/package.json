  "scripts": {
    "test": "jest",
    "dev": "VITE_BUILD_WATCH=true vite --config vite.config.dev.js",
    "prettier": "prettier --write './src/**/*.{js,ts,jsx,tsx}' '!course_flow/static/**/dist/**/*.{js,ts,jsx,tsx}'",
    "build": "tsc && vite --config vite.config.dev.js build",
    "tsc-watch": "tsc-watch --onSuccess \"npm run vite-build\"",
    "vite-build": "vite --config vite.config.dev.js build",
    "vite-watch": "wait-on dist && VITE_BUILD_WATCH=true vite --config vite.config.dev.js build --watch",
    "build-watch": "concurrently -k \"npm run tsc-watch\" \"npm run vite-watch\"",
    "buildDev": "tsc && vite build --mode development --minify false",
    "preview": "vite preview",
    "lint-quiet": "eslint --fix --quiet --max-warnings=-1  ./src --ext .js,.tsx,.ts -c ./.eslintrc.json"
  },
  "browserslist": "last 4 versions and >=0.2% in CA and not ie <= 11",
